@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:defaultScenarioConfig
  a o:ScenarioConfig ;
  o:randomSeed 1000 ;
  o:scenarioLength [ o:min 3; o:max 3; o:type xsd:integer] ;
  o:numScenarios 1 ;
  o:numWarmups 1 ;
  o:nfa [
    o:startState o:state1 ;
    o:transition [
      o:from o:state1 ;
      o:to o:state2 ;
      o:preventUndo true ;
      o:key "cp5" ;
      o:weight [ o:min 0.6 ; o:max 1.0 ]
    ] ;
    o:transition [
      o:from o:state2 ;
      o:to o:state2 ;
      o:key "cp1" ;
      o:weight [ o:min 0.6 ; o:max 1.0 ]
    ] ;
    o:transition [
      o:from o:state2 ;
      o:to o:state2 ;
      o:key "cp2" ;
      o:weight [ o:min 0.6 ; o:max 1.0 ]
    ] ;
    o:transition [
      o:from o:state2 ;
      o:to o:state2 ;
      o:key "cp3" ;
      o:weight [ o:min 0.6 ; o:max 1.0 ]
    ] ;
    o:transition [
      o:from o:state2 ;
      o:to o:state2 ;
      o:key "cp4" ;
      o:weight [ o:min 0.6 ; o:max 1.0 ]
    ] ;
    o:transition [
      o:from o:state2 ;
      o:to o:state2 ;
      o:key "cp5" ;
      o:weight [ o:min 0.6 ; o:max 1.0 ]
    ] ;
    o:transition [
      o:from o:state2 ;
      o:to o:state2 ;
      o:key "cp10" ;
      o:weight [ o:min 0.6 ; o:max 1.0 ]
    ] ;
    o:transition [
      o:from o:state2 ;
      o:to o:state2 ;
      o:key "cp11" ;
      o:weight [ o:min 0.6 ; o:max 1.0 ]
    ] ;
    o:transition [
      o:from o:state2 ;
      o:to o:state2 ;
      o:key "cp12" ;
      o:weight [ o:min 0.6 ; o:max 1.0 ]
    ] ;
    o:transition [
      o:from o:state2 ;
      o:to o:state2 ;
      o:key "cp13" ;
      o:weight [ o:min 0.6 ; o:max 1.0 ]
    ] ;
  ]
  .


